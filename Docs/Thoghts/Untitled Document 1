class Color
{
	pub var R = null
	pub byte G = null
	pub int B = null
	pub func Value() 
	{
		return [R,G,B]
	}
	Color()
	{
		 R = 0
		 G = 0
		 B = 0
	}
	Color(r,g,b)
	{
		R=r
		G=g
		B=b
	}
}

